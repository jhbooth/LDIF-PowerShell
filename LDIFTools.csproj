    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>
      James Booth
    </Authors>
    <Company>
      1765455 Ontario Inc., O/A BoothBilt
    </Company>
    <Copyright>
      Copyright Â© 2011-2022, BoothBilt - James Booth
    </Copyright>
    <NeutralLanguage>en</NeutralLanguage>
    <Version>
      2.1.0.0
    </Version>
    <AssemblyName>LDIFTools</AssemblyName>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="ReadLdif.psm1" />
  </ItemGroup>
  <ItemGroup>
